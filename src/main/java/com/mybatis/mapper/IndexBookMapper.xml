<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.dao.IndexBookMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.entity.IndexBook">
        <id column="id" property="id" />
        <result column="name" property="name" />
        <result column="level" property="level" />
        <result column="time" property="time" />
        <result column="book_img" property="bookImg" />
        <result column="direction" property="direction" />
        <result column="teacher" property="teacher" />
        <result column="price" property="price" />
        <result column="student_num" property="studentNum" />
        <result column="rate" property="rate" />
        <result column="knowledge_point_num" property="knowledgePointNum" />
        <result column="status" property="status" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, name, level, time, book_img, direction, teacher, price, student_num, rate, knowledge_point_num, status
    </sql>

</mapper>
